{
  "manifest_version": 3,
  "name": "Fix twitter icon",
  "description": "Based Extension",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["*://*.twitter.com/*"],
    "js": ["content.js"]
  }],
  "action": {
    "default_popup": "twitterFix.html",
    "default_icon": "swan.svg" 
  },
  "permissions" :  ["storage", "declarativeContent", "activeTab", "scripting"]
}
