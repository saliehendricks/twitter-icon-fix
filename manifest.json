{
  "manifest_version": 3,
  "name": "Fix twitter icon",
  "description": "Based Extension",
  "version": "1.0.1", 
  "permissions": [
    "storage"
  ], 
  "content_scripts": [{
    "matches": ["*://*.twitter.com/*"],
    "js": ["content.js"]
  }],
  "action": {
    "default_popup": "options.html"
  }
}
